nodes:
  indexer:
    - name: indexer-<%= @indexer_node1_name %>
      ip: <%= @node1host %>
    - name: indexer-<%= @indexer_node2_name %>
      ip: <%= @node2host %>
    - name: indexer-<%= @indexer_node3_name %>
      ip: <%= @node3host %>
  server:
    - name: manager-master
      ip: <%= @masterhost %>
      node_type: master
    - name: manager-worker
      ip: <%= @workerhost %>
      node_type: worker
  dashboard:
    - name: dashboard
      ip: <%= @dashboardhost %>